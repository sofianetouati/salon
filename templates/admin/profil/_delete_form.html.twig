<form method="post" action="{{ path('admin_delete', {'id': admin.id}) }}" onsubmit="return confirm('Voulez-vous vraiement supprimer cet administrateur de votre liste?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ admin.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash-alt me-1"></i>Supprimer</button>
</form>
